{
  "name": "lambda-typescrip-express-sequelize",
  "version": "1.0.0",
  "description": "Muestra de proyecto para una res API usando typeScript Express Sequelize.",
  "main": "dis/index.js",
  "scripts": {
    "dev:lambda": "cp env/dev.env .env && serverless offline",
    "dev": "nodemon",
    "start": "tsc && node dist/index.js",
    "deploy": "cp env/prd.env .env && serverless deploy",
    "test": "npx ts-node --files -r tsconfig-paths/register ./spec",
    "git": "git add . && git commit -m \"first commit\" && git push"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "keywords": [],
  "author": {
    "user": "jesteban39",
    "name": "Esteban Quintero",
    "email": "juanesqb28@gmail.com"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "jet-logger": "^1.3.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.1",
    "sequelize": "^6.33.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.18",
    "@types/morgan": "^1.9.6",
    "aws-sdk": "^2.1467.0",
    "esbuild": "^0.19.4",
    "nodemon": "^3.0.1",
    "pg-hstore": "^2.3.4",
    "serverless-esbuild": "^1.26.1",
    "serverless-offline": "^13.1.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  }
}
